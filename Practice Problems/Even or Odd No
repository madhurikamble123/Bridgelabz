package com.bridelabzPractice;

import java.util.Scanner;
public class Even_or_odd {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x= sc.nextInt();
        if(x % 2 == 0)
            System.out.println(x + " is even");
        else
            System.out.println(x + " is odd");
    }
}
