package com.bridelabzPractice;
public class MinimumNoArray {
    public static void main(String[] args) {
        System.out.println("maximum number is :" + Min());
    }
    static int[] arr = {10, 5, 8, 9, 20};
    static int Min() {
        int min = arr[0];
        for (int i = 1; i < arr.length; i=i+1)
            if (arr[i] < min)
                min = arr[i];
        return min;

    }
}
