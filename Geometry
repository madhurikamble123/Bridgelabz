package com.bridelabzPractice;
public class Geometry {
    public static void main(String[] args) {
        int x1, x2, y1, y2;
        double dis;
        x1 = 3;
        y1 = 4;
        x2 = 4;
        y2 = 3;
        dis = Math.sqrt((x2 - x1) * (x2 - x1) + (y2 - y1) * (y2 - y1));
        System.out.println("distance between" + "(" + x1 + "," + y1 + ")," + "(" + x2 + "," + y2 + ") the output is " + dis);

    }
}
